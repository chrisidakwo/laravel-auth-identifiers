{
  "name": "chrisidakwo/laravel-custom-auth",
  "type": "library",
  "description": "Laravel custom authentication with an option to use a custom password validation implementation",
  "keywords": ["admin", "custom", "authentication", "password", "custom", "validator", "laravel", "php"],
  "license": "GPL-3.0",
  "authors": [
    {
      "name": "Ekojoka Christolight Idakwo",
      "email": "chris.idakwo@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "illuminate/auth": "^5.8 || ^6.0 || ^7.0"
  },
  "extra": {
    "laravel-custom-auth": {
      "dont-discover": []
    }
  },
  "autoload": {
    "psr-4": {
      "ChrisIdakwo\\Auth\\": "src/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
